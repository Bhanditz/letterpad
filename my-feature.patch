++    } else if (
++        (matched = currentLineText.match(
++            /\[([^\]]+)\]\(([^\s)]+(?:[\t ]+"(?:\\.|[^"\\])*")?)\)/
++        ))
++    ) {
++        // [example](http://example.com "Optional title")
++        return matchLink("link", currentTextNode, matched, change);
++    }
++
++    if (lastChar === "*" || lastChar === "_") {
++        if (
++            (matched = prevTextFromSpace.match(
